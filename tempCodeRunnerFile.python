import numpy as np

def generate_spd_matrix(n):
    # Генерируем случайную симметричную матрицу
    matrix = np.random.rand(n, n)
    symmetric_matrix = (matrix + matrix.T) / 2
    # Генерируем симметричную положительную матрицу
    spd_matrix = symmetric_matrix + n * np.identity(n)
    return np.round(spd_matrix, decimals=2)

n = 4  # Размер матрицы
spd_matrix = generate_spd_matrix(n)
print("Сгенерированная симметричная положительная матрица:")
print(spd_matrix)
